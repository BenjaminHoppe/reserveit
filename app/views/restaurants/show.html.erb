restaurant: @restaurant %>
<% end %>
<ul>
  <% @menu_item.each do |menu_item| %>
  <%= render 'menu_item',
  menu_item: @menu_item %>
  <% end %>
</ul>
<ul>
  <% @reservation.each do |reservation| %>
  <%= render 'reservation',
  reservation: @reservation %>
  <% end %>
</ul>
